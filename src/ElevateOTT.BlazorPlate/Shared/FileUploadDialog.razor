<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">
            <MudIcon Icon="@Icons.Material.Filled.DeleteForever" Class="mr-3 mb-n1" />
            @Resource.File_Rename
        </MudText>
    </TitleContent>
    <DialogContent>
        <MudTextField Label="@Resource.File_Name"
                      Variant="Variant.Outlined"
                      @bind-Value="@FileName"
                      For="@(() => FileName)" />
        <MudAlert Severity="Severity.Info">@Resource.Please_provide_new_name_for_the_file_or_leave_it_as_is</MudAlert>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="RenameFile" Style="float: right" Variant="Variant.Filled" Color="Color.Error">@Resource.Submit</MudButton>
        <MudButton OnClick="Cancel" Color="Color.Default" Style="float: right" Class="mr-3" Variant="Variant.Filled">@Resource.Cancel</MudButton>
    </DialogActions>
</MudDialog>

